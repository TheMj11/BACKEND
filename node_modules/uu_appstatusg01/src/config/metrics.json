{
  "meterProviderMap": {
    "uuApp": {
      "readers": [
        {
          "name": "uuAppStatusMetricReader",
          "realization": "defaultPeriodicExportingMetricReader",
          "exporter": {
            "realization": "uu_appstatusg01:components/ProgressMetricExporter"
          },
          "exportIntervalMillis": "uu_app_status_metrics_progress_update_period_ms",
          "exportIntervalMillisDefault": 20000,
          "instrumentNameIncludeList": [
            "uuAwsUseCase\\..*",
            "uuAsidUseCase\\..*",
            "uuAppRuntime\\..*",
            "uuAwsAuthn\\..*"
          ]
        }
      ]
    }
  },
  "meterMap": {
    "uuAwsUseCase": {
      "options": {
        "attributeKeys": ["awid"]
      },
      "instrumentList": [
        {
          "type": "observableUpDownCounter",
          "name": "CallCount",
          "description": "Number of calls",
          "unit": "number",
          "valueType": "int"
        },
        {
          "type": "observableUpDownCounter",
          "name": "CallCountPm",
          "description": "Number of calls per minute",
          "unit": "number",
          "valueType": "int"
        },
        {
          "type": "observableUpDownCounter",
          "name": "CallCountPh",
          "description": "Number of calls per hour",
          "unit": "number",
          "valueType": "int"
        },
        {
          "type": "observableUpDownCounter",
          "name": "CallCountPd",
          "description": "Number of calls per day",
          "unit": "number",
          "valueType": "int"
        },
        {
          "type": "observableUpDownCounter",
          "name": "CallCountPw",
          "description": "Number of calls per week",
          "unit": "number",
          "valueType": "int"
        },
        {
          "type": "observableUpDownCounter",
          "name": "CallCountPmo",
          "description": "Number of calls per month",
          "unit": "number",
          "valueType": "int"
        },
        {
          "type": "observableUpDownCounter",
          "name": "CallCountTotal",
          "description": "Number of calls",
          "unit": "number",
          "valueType": "int"
        },
        {
          "type": "observableUpDownCounter",
          "name": "CallCountPmTotal",
