          "description": "Number of calls per hour",
          "unit": "number",
          "valueType": "int"
        },
        {
          "type": "observableUpDownCounter",
          "name": "CallCountPdTotal",
          "description": "Number of calls per day",
          "unit": "number",
          "valueType": "int"
        },
        {
          "type": "observableUpDownCounter",
          "name": "CallCountPwTotal",
          "description": "Number of calls per week",
          "unit": "number",
          "valueType": "int"
        },
        {
          "type": "observableUpDownCounter",
          "name": "CallCountPmoTotal",
          "description": "Number of calls per month",
          "unit": "number",
          "valueType": "int"
        }
