    "uuAppRuntime": {
      "instrumentList": [
        {
          "type": "observableGauge",
          "name": "MemoryRss",
          "description": "The amount of space occupied in the main memory device.",
          "unit": "B",
          "valueType": "int"
        },
        {
          "type": "observableGauge",
          "name": "MemoryHeapTotal",
          "description": "The total amount of V8 memory.",
          "unit": "B",
          "valueType": "int"
        },
        {
          "type": "observableGauge",
          "name": "MemoryHeapUsed",
          "description": "The amount of used V8 memory.",
          "unit": "B",
          "valueType": "int"
        },
        {
          "type": "observableGauge",
          "name": "MemoryExternal",
          "description": "The memory usage of C++ objects bound to JavaScript objects managed by V8.",
          "unit": "B",
          "valueType": "int"
        },
        {
          "type": "observableGauge",
          "name": "MemoryArrayBuffers",
          "description": "The memory allocated for ArrayBuffers and SharedArrayBuffers, including all Node.js Buffers.",
          "unit": "B",
          "valueType": "int"
        }
      ]
    },
